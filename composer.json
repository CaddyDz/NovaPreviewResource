{
	"name": "caddydz/nova-preview-resource",
	"description": "Preview a Laravel Nova resource via a popup on the index view",
	"keywords": [
		"laravel",
		"nova"
	],
	"authors": [
		{
			"name": "Salim Djerbouh",
			"email": "caddydz4@gmail.com",
			"homepage": "https://caddydz.github.io"
		}
	],
	"license": "MIT",
	"require": {
		"php": "^7.4|^8",
		"laravel/nova": "^4"
	},
	"autoload": {
		"psr-4": {
			"Caddydz\\NovaPreviewResource\\": "src/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Caddydz\\NovaPreviewResource\\FieldServiceProvider"
			]
		}
	},
	"config": {
		"sort-packages": true
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
